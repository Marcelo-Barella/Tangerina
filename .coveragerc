[run]
source = chatbot,features,flask_routes
omit =
    */tests/*
    */deploy/*
    */__pycache__/*
    .venv/*
    */venv/*

[report]
precision = 2
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
